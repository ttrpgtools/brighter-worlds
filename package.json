		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6"
	},
	"overrides": {
		"@rgossiaux/svelte-headlessui": {
			"svelte": "$svelte"
